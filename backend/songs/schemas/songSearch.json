{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.song.com/songSearch.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1
    },
    "dir": {
      "type": "string",
      "minLength": 1
    },
    "genre": {
      "type": "string",
      "enum": [
        "rock",
        "alt rock",
        "tv",
        "pop",
        "movie",
        "disco",
        "hip hop",
        "video game"
      ]
    }
  },
  "additionalProperties": false,
  "required": []
}